{
  "name": "pomodoro",
  "version": "2.1.0",
  "description": "Pomodoro timer build with electron",
  "license": "MIT",
  "repository": "https://github.com/G07cha/pomodoro.git",
  "author": "g07cha",
  "engines": {
    "node": ">=6"
  },
  "electronVersion": "1.7.8",
  "scripts": {
    "postinstall": "bower install --allow-root",
    "pretest": "electron-packager ./ --platform=darwin --arch=x64 --out=dist --overwrite",
    "test": "ava",
    "start": "electron .",
    "release": "build --ia32 --x64 --draft",
    "lint": "eslint ."
  },
  "dependencies": {
    "auto-launch": "^5.0.1",
    "human-readable-time": "^0.2.4",
    "jquery": "^3.2.1",
    "menubar": "^5.2.0",
    "photon": "git+https://github.com/connors/photon.git",
    "retinajs": "^2.1.0",
    "timer-stopwatch-dev": "^1.1.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "bower": "^1.8.2",
    "devtron": "^1.4.0",
    "electron": "^1.7.11",
    "electron-builder": "^12.3.0",
    "electron-packager": "^8.5.1",
    "electron-prebuilt": "^1.4.13",
    "eslint": "^3.16.1",
    "spectron": "^3.5.0"
  },
  "build": {
    "productName": "Pomodoro",
    "appId": "com.g07cha.pomodoro",
    "category": "public.app-category.productivity",
    "mac": {
      "icon": "build/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico",
      "iconUrl": "https://github.com/G07cha/pomodoro/blob/master/icons/icon.ico?raw=true"
    }
  }
}

{
  "name": "pomodoro",
  "version": "2.2.1",
  "description": "Pomodoro timer build with electron",
  "license": "MIT",
  "repository": "https://github.com/G07cha/pomodoro.git",
  "author": "g07cha",
  "engines": {
    "node": ">=6"
  },
  "electronVersion": "1.7.8",
  "scripts": {
    "postinstall": "bower install --allow-root",
    "pretest": "electron-packager ./ --platform=darwin --arch=x64 --out=dist --overwrite",
    "test": "ava",
    "start": "electron .",
    "release": "build --ia32 --x64 --mac",
    "lint": "eslint ."
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "electron-window-state": "^4.1.1",
    "human-readable-time": "^0.3.0",
    "jquery": "^3.3.1",
    "menubar": "^5.2.0",
    "photon": "git+https://github.com/connors/photon.git",
    "retinajs": "^2.1.3",
    "timer-stopwatch-dev": "^1.1.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "bower": "^1.8.2",
    "devtron": "^1.4.0",
    "electron": "^1.8.4",
    "electron-builder": "^20.11.1",
    "electron-packager": "^12.0.1",
    "electron-prebuilt": "^1.4.13",
    "eslint": "^4.19.1",
    "spectron": "^3.8.0"
  },
  "build": {
    "productName": "Pomodoro",
    "appId": "com.g07cha.pomodoro",
    "publish": {
      "provider": "github",
      "releaseType": "draft"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "build/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  }
}
